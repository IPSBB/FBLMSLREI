        <!-- Purchase Orders -->
        <!-- NAFTA Electric Generator Purchase Order -->
        <record id="demo_po_nafta" model="purchase.order">
            <field name="partner_id" ref="demo_vendor_canada"/>
            <field name="user_id" ref="base.user_admin"/>
            <field name="state">purchase</field>
            <field name="date_order" eval="(datetime.now() - relativedelta(days=15)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="date_planned" eval="(datetime.now() - relativedelta(days=10)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="company_id" ref="base.main_company"/>
            <field name="name">DEMO-PO-NAFTA</field>
            <field name="currency_id" ref="base.CAD"/>
        </record>

        <record id="demo_po_line_nafta" model="purchase.order.line">
            <field name="order_id" ref="demo_po_nafta"/>
            <field name="product_id" model="product.product" eval="obj().search([('product_tmpl_id', '=', ref('demo_product_motor_nafta'))], limit=1).id"/>
            <field name="name">NAFTA Electric Generator</field>
            <field name="product_qty">50</field>
            <field name="product_uom" ref="uom.product_uom_unit"/>
            <field name="price_unit">120.00</field>
            <field name="date_planned" eval="(datetime.now() - relativedelta(days=10)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="currency_id" ref="base.CAD"/>
        </record>

